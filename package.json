{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "webpack",
    "start:local": "ts-node src/server.ts",
    "migrations:generate": "ts-node src/db/migrations/generate.ts",
    "migrations:run": "ts-node src/scripts/migrate.ts",
    "migrations:run-force": "ts-node src/scripts/migrate.ts --force --seed",
    "migrations:seed": "ts-node src/scripts/seed.ts",
    "test": "npm run test:types && npm run test:logic",
    "test:logic": "mocha 'src/**/*.spec.ts'",
    "test:unit": "yarn run test:logic -- -g '@Unit'",
    "test:integration": "yarn run test:logic -- -g '@Integration'",
    "test:types": "tsc --noEmit"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.3.1",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.3.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "^9.1.1",
    "@types/slonik": "^22.1.5",
    "@types/uuid": "^8.3.4",
    "chai": "^4.3.6",
    "dotenv": "^16.0.0",
    "mocha": "^10.0.0",
    "mocha-dd-reporter": "^1.1.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.79.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cognito-express": "^3.0.1",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "helmet": "^5.0.2",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "ramda": "^0.28.0",
    "reflect-metadata": "^0.1.13",
    "slonik": "^28.1.0",
    "slonik-interceptor-preset": "^1.2.10",
    "uuid": "^8.3.2",
    "winston": "^3.7.2",
    "zod": "^3.14.4"
  }
}
